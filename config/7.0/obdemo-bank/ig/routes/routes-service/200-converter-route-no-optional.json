{
  "name": "200 - Converter no optional",
  "auditService": "AuditService-OB-Route",
  "baseURI": "https://&{identity.platform.fqdn}",
  "condition": "${matches(request.uri.path, '^/repo/converter/no/optional')}",
  "handler": {
    "type": "Chain",
    "config": {
      "filters": [
        {
          "name": "IntentConverterFilter-AccountAccessConsent",
          "type": "IntentConverterFilter",
          "config": {
            "messageType": "request",
            "resultTo": ["response"]
          }
        }
      ],
      "handler": {
        "type": "StaticResponseHandler",
        "config": {
          "status": 200,
          "reason": "OK"
        }
      }
    }
  }
}
