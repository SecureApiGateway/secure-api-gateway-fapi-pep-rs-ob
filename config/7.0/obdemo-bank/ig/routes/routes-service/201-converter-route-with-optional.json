{
  "name": "201 - Converter with optional",
  "auditService": "AuditService-OB-Route",
  "baseURI": "https://&{identity.platform.fqdn}",
  "condition": "${matches(request.uri.path, '^/repo/converter/with/optional')}",
  "handler": {
    "type": "Chain",
    "config": {
      "filters": [
        {
          "name": "IntentConverterFilter-AccountAccessConsent",
          "type": "IntentConverterFilter",
          "config": {
            "messageType": "request",
            "resultTo": [
              "RESPONSE",
              "REQUEST"
            ]
          }
        }
      ],
      "handler": {
        "type": "StaticResponseHandler",
        "config": {
          "status": 404,
          "reason": "Not Found"
        }
      }
    }
  }
}
